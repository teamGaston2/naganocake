<h1>商品一覧</h1>
<span>全<%= @items.count %>件</span>
<div class="items_index">
  <% @items.each do |f| %>
    <%= link_to(image_tag(f.get_image), item_path) %>
    <!--book.user.get_profile_image-->
    <%= image_tag f.get_profile_image(50, 50) %>
    <%= f.text_name %>
    <%= f.price %>
  <% end %>
</div>